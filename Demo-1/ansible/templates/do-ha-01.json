{
    "schemaVersion": "1.0.0",
    "class": "Device",
    "async": true,
    "label": "Onboard BIG-IP",
    "Common": {
       "class": "Tenant",
       "hostname": "bigip1.f5demo.cloud",
        "myDns": {
            "class": "DNS",
            "nameServers": [
                "8.8.8.8"
            ]
        },
        "myNtp": {
            "class": "NTP",
            "servers": [
                "0.pool.ntp.org",
                "1.pool.ntp.org",
                "2.pool.ntp.org"
            ],
            "timezone": "UTC"
        },		
		"myProvisioning": {
            "class": "Provision",
            "ltm": "nominal",
            "asm": "nominal"
        },
        "configsync": {
            "class": "ConfigSync",
            "configsyncIp": "/Common/internal-self/address"
        },
        "trust": {
            "class": "DeviceTrust",
            "localUsername": "kostas",
            "localPassword": "Kostas123",
            "remoteHost": "10.10.10.5",
            "remoteUsername": "kostas",
            "remotePassword": "Kostas123"
        },
        "failoverGroup": {
            "class": "DeviceGroup",
            "type": "sync-failover",
            "members": ["10.10.10.4", "10.10.10.5"],
            "owner": "10.10.10.5",
            "autoSync": true,
            "saveOnAutoSync": false,
            "networkFailover": true,
            "fullLoadOnSync": false,
            "asmSync": true
        },
		"managementRoute": [
			{
				"class": "ManagementRoute",
				"gw": "10.10.10.1",
				"network": "169.254.169.254"
			},
			{
				"class": "dhclient_route1",
				"gw": "10.10.10.1",
				"network": "168.63.129.16"
			},
			{
				"class": "default",
				"gw": "10.10.10.1",
				"network": "default"
			}
		],        
        "external": {
            "class": "VLAN",
            "tag": 4094,
            "mtu": 1500,
            "interfaces": [
                {
                    "name": "1.1",
                    "tagged": false
                }
            ]
        },
        "external-localself": {
            "class": "SelfIp",
            "address": "10.1.10.4/24",
            "vlan": "external",
            "allowService": "default",
            "trafficGroup": "traffic-group-local-only"
        },
        "internal": {
            "class": "VLAN",
            "tag": 4093,
            "mtu": 1500,
            "interfaces": [
                {
                    "name": "1.2",
                    "tagged": false
                }
            ]
        },
        "internal-self": {
            "class": "SelfIp",
            "address": "10.1.20.4/24",
            "vlan": "internal",
            "allowService": "default",
            "trafficGroup": "traffic-group-local-only"
        },
        "default": {
            "class": "Route",
            "gw": "10.1.10.1",
            "network": "default",
            "mtu": 1500
        },          
        "dbvars": {
            "class": "DbVariables",
			"config.allow.rfc3927": "enable",
            "ui.advisory.enabled": true,
            "ui.advisory.color": "green",
            "ui.advisory.text": "DO Through Ansible",
            "restjavad.useextramb": true,
            "provision.extramb": "1026"
        }
	}
}
